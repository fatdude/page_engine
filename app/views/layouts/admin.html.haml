!!!
%html
  %head
    %title PageEngine Admin
    - if rails_version == '3.1'
      = stylesheet_link_tag "application"
      = javascript_include_tag "application"
      = csrf_meta_tags   
    - else
      = stylesheet_link_tag :page_engine
      = javascript_include_tag 'jquery.min', :page_engine
      = csrf_meta_tag
  %body    
    #content
      = yield
